{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "drivnbd/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "buildCommand": "python manage.py collectstatic --noinput",
  "routes": [
    { "src": "/static/(.*)", "dest": "drivnbd/wsgi.py" },
    { "src": "/(.*)", "dest": "drivnbd/wsgi.py" }
  ]
}